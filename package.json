{
  "name": "vite-sandbox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src/server --ext js,mjs,cjs,json,ts,tsx --exec npm run start:server",
    "start:server": "node --loader ts-node/esm src/server/index.ts",
    "build": "run-p build:client:web build:client:ssr",
    "build:client:web": "vite build --outDir dist/client",
    "build:client:ssr": "vite build --outDir dist/server --ssr src/client/entry-server.tsx",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "packageManager": "pnpm@8.15.7",
  "dependencies": {
    "express": "4.19.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.12.7",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.57.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.6",
    "nodemon": "3.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.2.2",
    "vite": "5.2.0"
  }
}
